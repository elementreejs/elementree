{
  "name": "elementree",
  "version": "0.25.20",
  "description": "MV* Framework with Proxy Observables and JS Template Literals",
  "main": "dist/elementree.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "run-s clean test bundle size",
    "bundle": "webpack",
    "clean": "rimraf dist",
    "lint": "standard --fix",
    "release:patch": "npm run build && npm version patch && npm publish",
    "release:minor": "npm run build && npm version minor && npm publish",
    "release:major": "npm run build && npm version major && npm publish",
    "size": "size-limit",
    "test": "run-s lint test-run",
    "test-run": "browserify test.js | tape-run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elementreejs/elementree.git"
  },
  "author": "Mark Stahl <mjstahl@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/elementreejs/elementree/issues"
  },
  "homepage": "https://github.com/elementreejs/elementree#readme",
  "keywords": [
    "client",
    "minimal",
    "frontend",
    "framework",
    "html",
    "observables",
    "template",
    "template strings",
    "small",
    "tiny"
  ],
  "devDependencies": {
    "browserify": "^16.3.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.3",
    "size-limit": "^1.3.7",
    "standard": "^12.0.1",
    "tape": "^4.11.0",
    "tape-run": "^6.0.0",
    "webpack": "^4.35.3",
    "webpack-cli": "^3.3.6"
  },
  "dependencies": {
    "nanohtml": "^1.6.3",
    "nanomorph": "^5.4.0",
    "on-change": "^1.6.2",
    "onpushstate": "^0.3.1"
  },
  "size-limit": [
    {
      "path": "src/elementree.js",
      "limit": "6.5 KB"
    }
  ],
  "standard": {
    "ignore": [
      "/examples/"
    ]
  }
}
