{
  "name": "elementree",
  "version": "0.11.1",
  "description": "Finite State Machines + JS Template Literals",
  "main": "dist/elementree.js",
  "module": "index.js",
  "scripts": {
    "build": "npm run test && webpack && npm run size",
    "lint": "standard --fix",
    "release:patch": "npm version patch && npm run build && npm publish",
    "release:minor": "npm version minor && npm run build && npm publish",
    "release:major": "npm version major && npm run build && npm publish",
    "size": "size-limit",
    "test": "browserify test/test.js test/state.js | tape-run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elementreejs/elementree.git"
  },
  "author": "Mark Stahl <mjstahl@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/elementreejs/elementree/issues"
  },
  "homepage": "https://github.com/elementreejs/elementree#readme",
  "keywords": [
    "client",
    "minimal",
    "frontend",
    "framework",
    "state machine",
    "fsm",
    "finite state machine",
    "finite automata",
    "pda",
    "pushdown automata",
    "html",
    "template",
    "template strings",
    "small",
    "tiny"
  ],
  "devDependencies": {
    "browserify": "^16.2.3",
    "size-limit": "^0.21.1",
    "standard": "^12.0.1",
    "tape": "^4.10.1",
    "tape-run": "^6.0.0",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1"
  },
  "dependencies": {
    "@mjstahl/stated": "^6.1.3",
    "lodash.camelcase": "^4.3.0",
    "lodash.capitalize": "^4.2.1",
    "nanohtml": "^1.4.0",
    "nanomorph": "^5.2.0"
  },
  "size-limit": [
    {
      "path": "dist/elementree.js",
      "limit": "8 KB"
    }
  ]
}
