{
  "name": "elementree",
  "version": "0.20.0",
  "description": "Observables + JS Template Literals",
  "main": "dist/elementree.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "run-s lint test size",
    "bundle": "webpack",
    "clean": "rimraf dist",
    "lint": "standard --fix",
    "release:patch": "npm run build && npm version patch && npm publish",
    "release:minor": "npm run build && npm version minor && npm publish",
    "release:major": "npm run build && npm version major && npm publish",
    "size": "size-limit",
    "test": "run-s clean bundle test-bundle",
    "test-bundle": "browserify test/test.js | tape-run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elementreejs/elementree.git"
  },
  "author": "Mark Stahl <mjstahl@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/elementreejs/elementree/issues"
  },
  "homepage": "https://github.com/elementreejs/elementree#readme",
  "keywords": [
    "client",
    "minimal",
    "frontend",
    "framework",
    "html",
    "observables",
    "template",
    "template strings",
    "small",
    "tiny"
  ],
  "devDependencies": {
    "browserify": "^16.2.3",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.3",
    "size-limit": "^1.3.5",
    "standard": "^12.0.1",
    "tape": "^4.10.2",
    "tape-run": "^6.0.0",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2"
  },
  "dependencies": {
    "nanohtml": "^1.6.2",
    "nanomorph": "^5.4.0",
    "on-change": "github:sindresorhus/on-change"
  },
  "size-limit": [
    {
      "path": "dist/elementree.js",
      "limit": "5 KB"
    }
  ]
}
