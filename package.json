{
  "name": "elementree",
  "version": "0.7.7",
  "description": "Finite State Machines + JS Template Literals",
  "main": "dist/elementree.js",
  "module": "index.js",
  "scripts": {
    "build": "webpack && npm run size",
    "develop": "parcel examples/*html",
    "lint": "standard --fix index.js test/**/*.js",
    "size": "size-limit",
    "test": "npm run lint && ava"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mjstahl/elementree.git"
  },
  "author": "Mark Stahl <mjstahl@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mjstahl/elementree/issues"
  },
  "homepage": "https://github.com/mjstahl/elementree#readme",
  "devDependencies": {
    "ava": "^1.1.0",
    "browser-env": "^3.2.5",
    "parcel-bundler": "^1.11.0",
    "size-limit": "^0.21.1",
    "standard": "^12.0.1",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1"
  },
  "dependencies": {
    "@mjstahl/stated": "^6.1.3",
    "nanohtml": "^1.4.0",
    "nanomorph": "^5.1.3"
  },
  "ava": {
    "require": [
      "./test/helpers/browser-setup.js"
    ]
  },
  "size-limit": [
    {
      "path": "dist/elementree.js",
      "limit": "6 KB"
    }
  ]
}
