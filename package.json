{
  "name": "elementree",
  "version": "0.27.2",
  "description": "MV* Framework with Proxy Observables and JS Template Literals",
  "module": "dist/esm.js",
  "unpkg": "dist/esm.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "run-s clean lint bundle size",
    "bundle": "webpack",
    "clean": "rimraf dist",
    "lint": "standard --fix",
    "release:patch": "npm run build && npm version patch && npm publish",
    "release:minor": "npm run build && npm version minor && npm publish",
    "release:major": "npm run build && npm version major && npm publish",
    "size": "size-limit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elementreejs/elementree.git"
  },
  "author": "Mark Stahl <mjstahl@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/elementreejs/elementree/issues"
  },
  "homepage": "https://github.com/elementreejs/elementree#readme",
  "keywords": [
    "client",
    "minimal",
    "frontend",
    "framework",
    "html",
    "observables",
    "template",
    "template strings",
    "small",
    "tiny"
  ],
  "devDependencies": {
    "@purtuga/esm-webpack-plugin": "^1.1.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.3",
    "size-limit": "^1.3.7",
    "standard": "^12.0.1",
    "webpack": "^4.36.1",
    "webpack-cli": "^3.3.6"
  },
  "dependencies": {
    "nanohtml": "^1.6.3",
    "nanomorph": "^5.4.0",
    "on-change": "^1.6.2",
    "onpushstate": "^0.3.1"
  },
  "size-limit": [
    {
      "path": "./dist/esm.js",
      "limit": "6.1 KB"
    }
  ]
}
